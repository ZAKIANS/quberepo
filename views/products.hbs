<main role="main" class="col-md-9 ml-sm-auto col-lg-10 px-md-4">
    <section>
        <div class="container">


            <div class="h1 mt-4">
                Apk List
            </div>
            <a href="addproducts" class="btn btn-warning float-right my-2">Add Product</a>
            <div class="clearfix"></div>
            <table id="myTable" class="table table-bordered table-hover table-condensed table-export" cellspacing="0"
                width="100%">
                <thead class="bg-info">
                    <tr style="width:100%">
                        <th scope="col" style="width:5%">
                            #
                        </th>
                        <th scope="col" style="width:10%">
                            Image
                        </th>
                        <th scope="col" style="width:20%">
                            Title
                        </th>
                        <th scope="col" style="width:15%">
                            Category
                        </th>
                        <th scope="col" style="width:10%">
                            Date
                        </th>
                        <th scope="col" style="width:20%">
                            File
                        </th>
                        <th scope="col" style="width:5%">
                            Version
                        </th>
                        <th scope="col" style="width:15%">
                            Actions
                        </th>
                    </tr>
                </thead>
                <tbody>
                    {{#each products}}
                    <tr>
                        <td>
                            1 
                        </td>
                        <td class="center">
                            <a href=""><img class="rounded" src="assets/images/RoundPhoto_Nov132020_051024.png"
                                    width="40" height="40" /></a>
                        </td>
                        <td>
                            {{title}} </td>
                        <td>
                            {{category}} </td>
                        <td>
                            {{createdAt}}</td>
                        <td>
                            <a href="#">{{file}}</a>
                        </td>
                        <td>
                           {{{version
                           }}} </td>

                        <td>

                            <a href="" class="btn btn-circle btn-warning">Edit</a>
                            <a href="" class="btn btn-circle btn-danger">Del</a>
                        </td>
                    </tr>
                    {{/each}}
                   
                </tbody>
            </table>

        </div>
    </section>
</main>