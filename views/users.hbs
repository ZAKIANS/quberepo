            </th>
          </tr>
        </thead>
        <tbody>
           {{#each users}}
                    <tr>
                        <td>
                            {{this}}
                        </td>                        
                        <td>
                            {{name}}
                        </td>
                        <td>
                           {{email}}
                            </td>

                        <td>
